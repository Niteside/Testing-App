<h1>Listing ports</h1>
<div class='normal_table'>
<table>
  <tr>
    <th>Port</th>
    <th>Actionable</th>
    <th>Command</th>
    <th>Server</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ports.each do |port| %>
  <tr>
    <td><%= port.port %></td>
    <td><%= port.actionable %></td>
    <td><%= port.command %></td>
    <td><%= port.server_id %></td>
    <td><%= link_to 'Show', port %></td>
    <td><%= link_to 'Edit', edit_port_path(port) %></td>
    <td><%= link_to 'Destroy', port, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
  </div>
<br />

<%= link_to 'New Port', new_port_path %>
